<section class="pagination">
    <% if(hasPreviousPage) {%>
        <a href="?page=<%=previousPage%>"><</a>
    <%}%>                
    <a href="?page=<%=currentPage%>" class="active "><%=currentPage || 1%></a>
    <% if(hasNextPage) {%>
        <a href="?page=<%=nextPage%>">></a>
    <%}%>    
</section>